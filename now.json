{
	"version": 2,
	"builds": [{ "src": "index.js", "use": "@now/node-server" }],
	"routes": [
		{
		  "src": "/",
		  "dest": "/index.js",
		  "methods": ["GET"]
		},
		{
		  "src": "/ping",
		  "dest": "/index.js",
		  "methods": ["GET"]
		},
		{
		  "src": "/users",
		  "dest": "/index.js",
		  "methods": ["POST","GET","PUT","DELETE"]
		},
		{
			"src": "/tokens",
			"dest": "/index.js",
			"methods": ["POST","GET","PUT","DELETE"]
		  },
		  {
			"src": "/checks",
			"dest": "/index.js",
			"methods": ["POST","GET","PUT","DELETE"]
		  }
	  ]
}
